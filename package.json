{
    "name": "vue-compontent-scaffold",
    "version": "1.0.0",
    "private": true,
    "description": "a vue 2.0 compontent scaffold",
    "scripts": {
        "clean": "rimraf dist/*",
        "dev": "cross-env NODE_ENV=development node ./config/dev-server.js",
        "dev-server": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.conf.js --inline --hot --progress --colors",
        "prebuild": "npm run clean",
        "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.pro.conf.js --progress --hide-modules",
        "unit": "karma start ./config/webpack.unit.conf.js"
    },
    "keywords": [
        "vue",
        "vue 2.0",
        "components",
        "hot middleware",
        "eslint",
        "karma",
        "mocha"
    ],
    "author": "Ryan Nan",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^6.5.3",
        "babel-core": "^6.18.2",
        "babel-loader": "^6.2.7",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-stage-0": "^6.16.0",
        "chai": "^3.5.0",
        "cross-env": "^3.1.3",
        "css-loader": "^0.25.0",
        "element-ui": "^1.0.0",
        "es6-promise": "^4.0.5",
        "eslint": "^3.10.1",
        "eslint-config-vue": "^2.0.1",
        "eslint-loader": "^1.6.1",
        "eslint-plugin-vue": "^1.0.0",
        "eventsource-polyfill": "^0.9.6",
        "exports-loader": "^0.6.3",
        "express": "^4.14.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.9.0",
        "glob": "^7.1.1",
        "html-webpack-plugin": "^2.24.1",
        "http-proxy-middleware": "^0.17.2",
        "isparta-loader": "^2.0.0",
        "karma": "^1.3.0",
        "karma-coverage": "^1.1.1",
        "karma-mocha": "^1.3.0",
        "karma-phantomjs-launcher": "^1.0.2",
        "karma-sinon-chai": "^1.2.4",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-spec-reporter": "0.0.26",
        "karma-webpack": "^1.8.0",
        "mocha": "^3.1.2",
        "opn": "^4.0.2",
        "phantomjs-prebuilt": "^2.1.13",
        "postcss-cssnext": "^2.8.0",
        "postcss-loader": "^1.1.1",
        "precss": "^1.4.0",
        "sinon": "^1.17.6",
        "sinon-chai": "^2.8.0",
        "style-loader": "^0.13.1",
        "stylus": "^0.54.5",
        "stylus-loader": "^2.3.1",
        "url-loader": "^0.5.7",
        "vue-loader": "^9.9.1",
        "vue-resource": "^1.0.3",
        "vue-style-loader": "^1.0.0",
        "webpack": "^1.13.3",
        "webpack-dev-middleware": "^1.8.4",
        "webpack-dev-server": "^1.16.2",
        "webpack-hot-middleware": "^2.13.2",
        "webpack-merge": "^0.15.0"
    },
    "dependencies": {
        "animate.css": "^3.5.2",
        "font-awesome": "^4.7.0",
        "normalize.css": "^5.0.0",
        "vue": "^2.0.5",
